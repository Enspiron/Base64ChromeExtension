{
  "name": "Base64",
  "version": "1.0",
  "description": "Open Base64 Links",
  "manifest_version": 2,
  "content_scripts":[
    {
    "matches": ["<all_urls>"],
    "js": ["popup.js"],
    "css": ["popup.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Base64"
  }
}
